on.Out);local ok,err=pcall(function() local tw=TweenService:Create(hrp,tweenInfo,{CFrame=targetCFrame});tw:Play();tw.Completed:Wait();end);if  not ok then hrp.CFrame=targetCFrame;end wait(0.1);debounce=false;end mouse.Button1Down:Connect(function() if  not ctrlHeld then return;end local hit=mouse.Hit;if  not hit then return;end local pos=hit.p or hit.Position ;if  not pos then return;end teleportTo(pos);end);player.CharacterAdded:Connect(function(char) end);end});Section:NewToggle({Title="Hide Notification",Default=false,Callback=function(tr) print(tr);end});InfoSection:NewTitle("If you find any bugs or problems with the script, you can report the issue in Discord.");end);if  not ok then warn("เกิดข้อผิดพลาดขณะรันสคริปต์: ",err);end end
